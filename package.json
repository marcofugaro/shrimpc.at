{
  "name": "shrimpcat",
  "version": "1.0.0",
  "repository": "marcofugaro/shrimpcat",
  "author": "Marco Fugaro",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "postinstall": "yarn clean-browsersync",
    "clean-browsersync": "sed -i -e 's/sourceMappingURL/_/g' ./node_modules/browser-sync-client/dist/index.js",
    "start": "cross-env NODE_ENV=development node ./esbuild.js",
    "prebuild": "yarn clean; yarn copy-public",
    "build": "cross-env NODE_ENV=production node ./esbuild.js",
    "clean": "rimraf build/; rimraf public/app.js; rimraf public/app.js.map",
    "copy-public": "cpr public/ build/"
  },
  "dependencies": {
    "@tweenjs/tween.js": "^17.2.0",
    "cannon": "^0.6.2",
    "canvas-sketch-util": "^1.6.0",
    "controls-gui": "^1.2.2",
    "controls-state": "^1.1.1",
    "datauritoblob": "^1.0.0",
    "detect-gpu": "^1.1.0",
    "eases": "^1.0.8",
    "image-promise": "^6.0.0",
    "lodash-es": "^4.17.21",
    "orbit-controls": "^1.2.4",
    "p-map": "^3.0.0",
    "pixi.js": "5.1.1",
    "pretty-ms": "^5.0.0",
    "shake.js": "^1.2.2",
    "stats.js": "^0.17.0",
    "three": "0.109.0",
    "touches": "^1.2.2",
    "web-audio-touch-unlock": "^1.0.1"
  },
  "devDependencies": {
    "address": "^1.2.2",
    "browser-sync": "^2.27.11",
    "chalk": "4.1.2",
    "cpr": "^3.0.1",
    "cross-env": "^7.0.3",
    "esbuild": "^0.16.10",
    "esbuild-plugin-glslify": "^1.0.1",
    "esbuild-plugin-glslify-inline": "^1.1.0",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "glslify": "^7.1.1",
    "indent-string": "^5.0.0",
    "ora": "^6.1.2",
    "react-dev-utils": "^12.0.1",
    "rimraf": "^3.0.2",
    "tree-node-cli": "^1.6.0"
  }
}